{
  "name": "burdadirect/symfony-bundle-async-worker",
  "description": "HBM Async Worker Bundle - Async execution of symfony commands via redis queues.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "HBM\\AsyncWorkerBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories" : [
    {
      "type": "vcs",
      "url": "git@github.com:burdadirect/symfony-bundle-twig-extensions.git"
    }
  ],
  "require": {
    "php" : ">=7.3",
    "ext-redis": "*",
    "ext-json": "*",
    "burdadirect/symfony-bundle-twig-extensions": "^1.0|dev-master",
    "psr/log": "^1.0",
    "symfony/config": "^4.4|^5.2",
    "symfony/console": "^4.4|^5.2",
    "symfony/dependency-injection": "^4.4|^5.2",
    "symfony/swiftmailer-bundle": "^3.2",
    "long-running/long-running": "^0.5",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit" : "^8.5|^9.5",
    "symfony/phpunit-bridge": "^4.4|^5.2"
  }
}
