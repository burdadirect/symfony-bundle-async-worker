{
  "name": "burdanews/symfony-bundle-async-worker",
  "description": "HBM Async Worker Bundle - Async execution of symfony commands via redis queues.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-0": {
      "HBM\\AsyncWorkerBundle": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories" : [
    {
      "type": "vcs",
      "url": "git@github.com:burdanews/symfony-bundle-twig-extensions.git"
    }
  ],
  "require": {
    "php" : ">=7.3",
    "ext-redis": "*",
    "ext-json": "*",
    "burdanews/symfony-bundle-twig-extensions": "dev-master",
    "psr/log": "^1.0",
    "symfony/config": "^3.4|^4.4",
    "symfony/console": "^3.4|^4.4",
    "symfony/dependency-injection": "^3.4|^4.4",
    "symfony/swiftmailer-bundle": "^3.2",
    "long-running/long-running": "^0.3.0",
    "twig/twig": "^2.11"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit" : "^6.5|^7.0",
    "symfony/phpunit-bridge": "^3.4|^4.4"
  },
  "target-dir": "HBM/AsyncWorkerBundle"
}
